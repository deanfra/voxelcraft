{
  "name": "minecraft-voxel-generator",
  "version": "0.0.1",
  "description": "Minecraft Voxel Generator",
  "main": "build/voxel.js",
  "files": [
    "voxel",
    "LICENSE",
    "package.json",
    "README.md",
    "build"
  ],
  "directories": {
    "voxel": "voxel"
  },
  "engines": {
    "node": ">=10.14"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": "mdcs",
    "plugins": [
      "html",
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": 1,
      "@typescript-eslint/indent": [
        "error",
        "tab",
        {
          "SwitchCase": 1
        }
      ],
      "prefer-const": [
        "error",
        {
          "destructuring": "any",
          "ignoreReadBeforeAssign": false
        }
      ]
    }
  },
  "scripts": {
    "start": "npm run dev",
    "test": "jest",
    "build": "rollup -c utils/build/rollup.config.js",
    "dev": "concurrently --names \"ROLLUP,HTTP\" -c \"bgBlue.bold,bgGreen.bold\" \"rollup -c utils/build/rollup.config.js -w -m inline\" \"http-server -c-1 -p 8080\"",
    "lint-fix": "eslint src --ext js --ext ts --ignore-pattern libs --fix"
  },
  "keywords": [
    "three",
    "three.js",
    "javascript",
    "3d",
    "webgl",
    "webgl2",
    "canvas",
    "html5"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-typescript": "^7.12.1",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.6.0",
    "eslint-config-mdcs": "^5.0.0",
    "eslint-plugin-html": "^6.0.3",
    "google-closure-compiler": "20200719.0.0",
    "http-server": "^0.12.3",
    "jest": "^26.4.2",
    "rollup": "^2.23.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^26.4.1",
    "typescript": "^3.9.7"
  },
  "jspm": {
    "files": [
      "package.json",
      "LICENSE",
      "README.md",
      "build/voxel.js",
      "build/index.html",
      "build/main.css"
    ],
    "directories": {}
  },
  "dependencies": {
    "three": "^0.121.1"
  }
}
